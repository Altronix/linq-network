{
  "name": "@altronix/linq-network-js",
  "version": "0.0.1-alpha-9",
  "description": "Node bindings for linq-network",
  "main": "./build/install/lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "native-build": "cmake-js --CDCMAKE_INSTALL_PREFIX=./ --CDBUILD_LINQD=OFF --CDBUILD_SHARED=OFF --CDWITH_NODEJS_BINDING --CDCMAKE_BUILD_TYPE=MinSizeRel build --target=install",
    "native-clean": "cmake-js clean",
    "build": "npm run native-build && tsc -p ./bindings/nodejs/lib",
    "install": "npm run native-build && tsc -p ./bindings/nodejs/lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomzBench/linq-network.git"
  },
  "author": "Altronix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomzBench/linq-network/issues"
  },
  "homepage": "https://github.com/TomzBench/linq-network#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.0.0",
    "nan": "^2.14.0",
    "node-addon-api": "^2.0.0",
    "@types/node": "^13.1.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {}
}
