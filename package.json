{
  "name": "@altronix/linq",
  "version": "0.0.4-rc.34",
  "description": "Node bindings for linq-network",
  "main": "./build/install/lib/index.js",
  "linq": {
    "usbh": true
  },
  "scripts": {
    "test": "jest --config=./bindings/nodejs/lib/jest.config.js",
    "native:cmake": "node ./scripts/build-nodejs-binding.js",
    "native:build": "yarn node-gyp build",
    "native:configure": "yarn node-gyp configure",
    "native:install": "yarn node-gyp install",
    "native:clean": "yarn node-gyp clean",
    "build": "tsc -p ./bindings/nodejs/lib",
    "build:examples": "tsc --build ./bindings/nodejs/examples/update/tsconfig.json",
    "build:integration-test": "tsc --build ./test/integration/timeout/tsconfig.json",
    "build:events": "tsc --build ./bindings/nodejs/examples/events",
    "run:events": "node ./bindings/nodejs/examples/events/index.js",
    "preinstall": "yarn run native:cmake",
    "postinstall": "yarn run native:configure && yarn run native:build && tsc -p ./bindings/nodejs/lib"
  },
  "files": [
    "/bindings/cpp",
    "/bindings/nodejs/lib",
    "/bindings/nodejs/prebuilds",
    "/bindings/nodejs/src",
    "/bindings/nodejs/CMakeLists.txt",
    "/cmake",
    "/dl/*",
    "/external/jsmn-web-tokens",
    "/external/libusb",
    "/libcommon",
    "/libdatabase",
    "/libhttp",
    "/libnetwork",
    "/libusb",
    "/scripts",
    "CMakeLists.txt",
    "COPYING",
    "package.json",
    "tsconfig.base.json",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomzBench/linq-network.git"
  },
  "author": "Altronix",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomzBench/linq-network/issues"
  },
  "homepage": "https://github.com/TomzBench/linq-network#readme",
  "dependencies": {
    "@types/node": "^14.14.22",
    "bindings": "^1.5.0",
    "nan": "^2.14.2",
    "node-addon-api": "^3.1.0",
    "rxjs": "^6.6.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "minimist": "^1.2.5",
    "node-gyp": "^7.1.2",
    "ts-jest": "^26.4.4"
  }
}
